<%- include("partials/header") -%>
<div class="main-cont">
    <h1>Compose</h1>


    <form action="/compose" method="post" class="cont">
        <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label">Title</label>
            <input type="text" name="title" class="form-control" id="exampleFormControlInput1">
        </div>
        <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">Post</label>
            <textarea class="form-control" name="post" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>

        <input class="addButton btn btn-dark" type="submit" value="Publish">
    </form>


</div>


<%- include("partials/footer") -%>